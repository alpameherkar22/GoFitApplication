<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login.main.FullBody"
    android:background="@drawable/background">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:id="@+id/banner"
        android:src="@drawable/colow"
        android:background="#ffffff"/>
    <View
        android:layout_width="wrap_content"
        android:layout_height="4dp"
        android:layout_below="@+id/banner"
        android:background="@color/colorPrimary" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ff9980"
        android:text="Full  Body  workouts"
        android:textStyle="bold"
        android:fontFamily="@font/oswaldstencil"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:textSize="25sp"
        android:id="@+id/avids"
        android:layout_below="@+id/banner"/>
    <View
        android:layout_width="wrap_content"
        android:layout_height="2dp"
        android:layout_below="@+id/avids"
        android:background="@color/colorPrimary" />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="430dp"
        android:layout_below="@+id/avids"
        android:id="@+id/rlay">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="400dp">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:layout_marginLeft="7dp"
                android:layout_marginTop="10dp"
                android:id="@+id/r4">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ff9980"
                    android:fontFamily="@font/titilliumsemiboldupright"
                    android:textSize="25sp"
                    android:id="@+id/l12"
                    android:text="This workout focuses on every part of the body, helping to reduce fats and weight loss.This workout provide  strength and healthy muscles overall the body."
                    />
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                android:layout_below="@+id/r4"
                android:background="@color/colorPrimary" />
            <RelativeLayout
                android:layout_centerHorizontal="true"
                android:layout_width="280dp"
                android:layout_height="155dp"
                android:background="@drawable/full1"
                android:id="@+id/ab1"
                android:layout_below="@+id/r4"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="20dp">
                <ImageButton
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_centerInParent="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:background="@drawable/changeselector"
                    android:id="@+id/t1"/>
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                android:layout_below="@+id/ab1"
                android:layout_marginTop="5dp"
                android:background="@color/colorPrimary" />
            <RelativeLayout
                android:layout_width="280dp"
                android:layout_centerHorizontal="true"
                android:layout_height="155dp"
                android:background="@drawable/full2"
                android:id="@+id/ab2"
                android:layout_below="@+id/ab1"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp">
                <ImageButton
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_centerInParent="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:background="@drawable/changeselector"
                    android:id="@+id/t2"/>
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                android:layout_below="@+id/ab2"
                android:layout_marginTop="5dp"
                android:background="@color/colorPrimary" />
            <RelativeLayout
                android:layout_width="280dp"
                android:layout_centerHorizontal="true"
                android:layout_height="155dp"
                android:background="@drawable/full3"
                android:id="@+id/ab3"
                android:layout_below="@+id/ab2"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="20dp">
                <ImageButton
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_centerInParent="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:background="@drawable/changeselector"
                    android:id="@+id/t3"/>
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                android:layout_below="@+id/ab3"
                android:layout_marginTop="5dp"
                android:background="@color/colorPrimary" />
            <RelativeLayout
                android:layout_width="280dp"
                android:layout_centerHorizontal="true"
                android:layout_height="155dp"
                android:background="@drawable/full4"
                android:id="@+id/ab4"
                android:layout_below="@+id/ab3"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp">
                <ImageButton
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_centerInParent="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:background="@drawable/changeselector"
                    android:id="@+id/t4"/>
            </RelativeLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                android:layout_below="@+id/ab4"
                android:layout_marginTop="5dp"
                android:background="@color/colorPrimary" />
            <RelativeLayout
                android:layout_width="280dp"
                android:layout_centerHorizontal="true"
                android:layout_height="155dp"
                android:background="@drawable/full5"
                android:id="@+id/ab5"
                android:layout_below="@+id/ab4"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="20dp">
                <ImageButton
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_centerInParent="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:background="@drawable/changeselector"
                    android:id="@+id/t5"/>
            </RelativeLayout>

            <View
                android:layout_width="wrap_content"
                android:layout_height="2dp"
                android:layout_below="@+id/ab5"
                android:layout_marginTop="5dp"
                android:background="@color/colorPrimary" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Flat abs / Sixpack abs  Workout"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/ab5"
                android:id="@+id/flat"/>
            <View
                android:layout_width="350dp"
                android:layout_height="1dp"
                android:layout_below="@+id/flat"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Leg / Glutes  Workout"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/flat"
                android:id="@+id/leg"/>
            <View
                android:layout_width="240dp"
                android:layout_height="1dp"
                android:layout_below="@+id/leg"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Slender Arm Workout"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/leg"
                android:id="@+id/arm"/>
            <View
                android:layout_width="240dp"
                android:layout_height="1dp"
                android:layout_below="@+id/arm"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Stretching"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/arm"
                android:id="@+id/stre"/>
            <View
                android:layout_width="125dp"
                android:layout_height="1dp"
                android:layout_below="@+id/stre"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />
        </RelativeLayout>
    </ScrollView>
    <View
        android:layout_width="wrap_content"
        android:layout_height="4dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/rlay"
        android:background="@color/colorPrimary" />
    <Button
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:id="@+id/back"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="20dp"
        android:text="BACK"
        android:fontFamily="@font/colablig"
        android:textSize="20dp"
        android:textColor="#696969"
        android:background="@drawable/buttonselector"
        android:layout_alignParentBottom="true"/>
    <Button
        android:id="@+id/next"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="170dp"
        android:layout_marginBottom="20dp"
        android:layout_toRightOf="@+id/back"
        android:background="@drawable/buttonselector"
        android:text="CONTACT US"
        android:fontFamily="@font/colablig"
        android:textColor="#696969"
        android:textSize="17dp" />

    <ImageView
        android:layout_width="87dp"
        android:layout_height="57dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="13dp"
        android:id="@+id/fea"
        android:background="@drawable/feather" />
</RelativeLayout>
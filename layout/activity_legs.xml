                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ff9980"
                    android:fontFamily="@font/titilliumsemiboldupright"
                    android:textSize="25sp"
                    android:id="@+id/l7"
                    android:layout_below="@+id/l6"
                    android:text="Types of lower body workouts:"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ff9980"
                    android:fontFamily="@font/titilliumsemiboldupright"
                    android:textSize="25sp"
                    android:id="@+id/l8"
                    android:layout_below="@+id/l7"

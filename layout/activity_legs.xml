                android:layout_marginTop="5dp"
                android:background="@color/colorPrimary" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Flat abs / Sixpack abs  Workout"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/ab10"
                android:id="@+id/flat"/>
            <View
                android:layout_width="350dp"
                android:layout_height="1dp"
                android:layout_below="@+id/flat"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Slender Arm Workout"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/flat"
                android:id="@+id/arm"/>
            <View
                android:layout_width="240dp"
                android:layout_height="1dp"
                android:layout_below="@+id/arm"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Full  Body  workouts"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/arm"
                android:id="@+id/full"/>
            <View
                android:layout_width="225dp"
                android:layout_height="1dp"
                android:layout_below="@+id/full"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Stretching"
                android:textColor="#ff9980"
                android:textStyle="bold"
                android:fontFamily="@font/oswaldstencil"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textSize="25sp"
                android:layout_below="@+id/full"
                android:id="@+id/stre"/>
            <View
                android:layout_width="125dp"
                android:layout_height="1dp"
                android:layout_below="@+id/stre"
                android:layout_marginLeft="10dp"
                android:background="@color/colorPrimaryDark" />

        </RelativeLayout>
    </ScrollView>
    <View
        android:layout_width="wrap_content"
        android:layout_height="4dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/rlay"
        android:background="@color/colorPrimary" />
    <Button
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:id="@+id/back"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="20dp"
        android:text="BACK"
        android:fontFamily="@font/colablig"
        android:textSize="20dp"
        android:textColor="#696969"
        android:background="@drawable/buttonselector"
        android:layout_alignParentBottom="true"/>
    <Button
        android:id="@+id/next"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="170dp"
        android:layout_marginBottom="20dp"
        android:layout_toRightOf="@+id/back"
        android:background="@drawable/buttonselector"
        android:text="CONTACT US"
        android:fontFamily="@font/colablig"
        android:textColor="#696969"
        android:textSize="17dp" />

    <ImageView
        android:layout_width="87dp"
        android:layout_height="57dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="13dp"
        android:id="@+id/fea"
        android:background="@drawable/feather" />
</RelativeLayout>


